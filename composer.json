        "drupal/shield": "^1.2",
        "drupal/single_datetime": "^1.7",
        "drupal/sitemap": "^1.5",
        "drupal/social_media_links": "^2.6",
        "drupal/soundcloudfield": "^1.0@alpha",
        "drupal/stage_file_proxy": "^1.0",
        "drupal/statistics_counter": "^1.0",
        "drupal/taxonomy_manager": "^1.0@alpha",
        "drupal/taxonomy_unique": "^2.0",
        "drupal/token": "^1.5",
        "drupal/twig_field_value": "^1.1",
        "drupal/twig_tweak": "^1.9",
        "drupal/twig_xdebug": "^1.1",
        "drupal/twitter_block": "3.x-dev",
        "drupal/ultimate_cron": "^2.0",
        "drupal/video_embed_field": "^2.2",
        "drupal/views_bootstrap": "^3.0",
        "drupal/views_bulk_edit": "^2.4",
        "drupal/views_bulk_operations": "^2.1",
        "drupal/views_infinite_scroll": "^1.7",
        "drupal/views_slideshow": "^4.6",
        "drupal/views_term_hierarchy_weight_field": "1.x-dev",
        "drupal/webform": "^5.5",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "~9.0",
        "sainsburys/guzzle-oauth2-plugin": "^3.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patches": {
          "drupal/entity_embed": {
            "Link images": "https://www.drupal.org/files/issues/2018-04-23/25114040-47.patch"
          },
         "drupal/soundcloudfield": {
            "Embed stopped working...": "https://www.drupal.org/files/issues/2019-08-15/soundcloud-oembed_https-3075203-2-D8.patch"
          }
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}

        "drupal/entity_reference_revisions": "^1.3",
        "drupal/entityqueue": "^1.0@beta",
        "drupal/environment_indicator": "^3.3",
        "drupal/eu_cookie_compliance": "^1.8",
        "drupal/facebook_tracking_pixel": "1.x-dev",
        "drupal/facets": "^1.4",
        "drupal/feeds_para_mapper": "1.x-dev",
        "drupal/feeds_tamper": "^2.0",
        "drupal/field_formatter_class": "^1.1",
        "drupal/field_group": "^3.0",
        "drupal/fieldblock": "^2.0@alpha",
        "drupal/file_browser": "^1.1",
        "drupal/flag": "^4.0",
        "drupal/flexslider": "^2.0@beta",
        "drupal/focal_point": "^1.3",
        "drupal/google_analytics": "^2.1",
        "drupal/honeypot": "^1.29",
        "drupal/image_styles_mapping": "^1.2",
        "drupal/image_widget_crop": "^2.2",
        "drupal/imce": "^1.7",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/layout_plugin": "^1.0@alpha",
        "drupal/link_attributes": "^1.10",
        "drupal/linkit": "^4.3",
        "drupal/masquerade": "^2.0",
        "drupal/matomo": "^1.6",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.1",
        "drupal/migrate_plus": "^4.2",
        "drupal/migrate_source_csv": "^2.2",
        "drupal/migrate_tools": "^4.4",
        "drupal/migrate_upgrade": "^3.0",
        "drupal/module_filter": "^3.1",
        "drupal/module_missing_message_fixer": "^1.1",
        "drupal/pathauto": "^1.0",

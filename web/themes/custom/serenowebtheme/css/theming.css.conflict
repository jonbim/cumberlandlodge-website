/*  Homepage styles */
.jumbotron { color:#fff; background-color:#3F2683; }
.jumbotron .h1, .jumbotron h1 { font-size:36px; }
.jumbotron .h4, .jumbotron h4 { color:#fff; font-size:22px; }
.jumbotron p { font-size:15px; }
.jumbotron label { display:none; }
.jumbotron .btn { margin-top:20px; }
.CTAholder, .newsholder, .eventsholder { display: flex; flex-direction: row; flex-wrap: wrap; }
.CTAbox, .newsbox, .eventsbox { display:flex; flex-direction:column; margin-bottom:30px; }
.CTAboxInner { background-color:#EC607E; display:flex; flex-direction:column; flex:1 0 auto; padding:15px 30px 25px 30px; align-items: center; }
.CTAbox h2, .CTAbox p { color:#fff; text-align:center; }
.CTAbox h2 { margin:10px 0 15px 0; font-size:30px; line-height:1em; }
.CTAbox p { font-size:16px; margin:0 0 5px 0; }
.CTAbox p a { text-decoration:underline; color:#fff; }
.CTAbox p a:hover { text-decoration:underline; color:#222; }
.CTAboxInner .icon { width:40px; }
.homeNews { border-top:1px solid #FDC1CE; padding-top:12px; margin-bottom:20px; }
.twitterFeedInner { border-left:1px solid #BCBCBC; padding-left:20px; margin-bottom:30px; }

.path-frontpage .landing-page div.paragraph h2 { text-align:left; }
.path-frontpage .landing-page .content .news-item, .path-frontpage .landing-page .content .event { margin-bottom: 20px; }
.path-frontpage .landing-page .content .news-item p, .path-frontpage .landing-page .content .event p { display:none; }
.path-frontpage .landing-page .content .news-item h3 a, .path-frontpage .landing-page .content .event h3 a, .path-frontpage .landing-page .content .event .glyphicon-calendar, .path-frontpage .landing-page .content .event .field--item { color:#fff; }
.path-frontpage .landing-page .content .news-item h3, .path-frontpage .landing-page .content .event h3 { margin-top: 0; }
.path-frontpage .landing-page .content .tags a, .path-frontpage .landing-page .content .tags .glyphicon { color:#FFCD00; }
.path-frontpage .landing-page .content .event .field--item { margin-bottom: 5px; font-weight: bold; }
.path-frontpage .landing-page .content .news-item h3 a:hover, .path-frontpage .landing-page .content .event h3 a:hover, .path-frontpage .landing-page .content .tags a:hover { color:#FFCD00; text-decoration: none; }
.path-frontpage .landing-page .content .tags a { margin-left:7px; }
.path-frontpage .landing-page .content .news-item hr, .path-frontpage .landing-page .content .event hr { margin-top: 0; margin-bottom: 10px; }

/*  News and Events box styles */
.newsandevents h2 { font-size:36px; padding-top:30px; padding-bottom:20px; }
.newsboxInner { background-color:#fff; display:flex; flex-direction:column; flex:1 0 auto; border-bottom-right-radius:25px; transition: background-color 0.5s; }
.newsboxText { padding:15px 30px 25px 30px; }
.newsboxText p { color:#3F2683; }
.newsboxText h3 { font-family: 'PT Sans', sans-serif; color:#2B2B2B; letter-spacing:0; margin: 0; }
.newsbox a:hover h3, .newsbox a:hover p { color:#fff; }
.newsboxInner:hover { background-color:#3F2683; }
.newsbox a:focus, .newsbox a:hover { text-decoration:none; }
.srn-flex {
  display: flex;
}
.eventsboxInner { background-color:#fff; border-top:3px solid #EC607E; padding:25px 30px 20px 30px; display:flex; flex-direction:column; flex:1 0 auto; border-bottom-right-radius:25px; transition: background-color 0.5s; }
.eventsboxInner h3 { font-family: 'PT Sans', sans-serif; font-weight:700; color:#EC607E; letter-spacing:0; margin: 0 0 10px 0; }
.eventsboxInner p { color:#2B2B2B; font-size:18px; }
.eventsboxInner:hover h3, .eventsboxInner:hover p, .eventsboxInner:hover p .salmon { color:#fff; }
.eventsboxInner:hover { background-color:#EC607E; }
.eventsboxInner:focus, .eventsboxInner:hover { text-decoration:none; }


/* Form pages */
.region-sidebar-first h2 { border-top:1px solid #EC607E; border-bottom:1px solid #EC607E; font-size: 30px; padding:7px 0 3px 0; color: #EC607E; margin-top: 0; }
.region-sidebar-first .facets-widget-checkbox { margin-bottom: 25px; }
.region-sidebar-first .facets-widget-checkbox ul { padding:0; margin:0; list-style:none; }
.region-sidebar-first .facets-widget-checkbox li { margin-bottom:5px;}
.region-sidebar-first .facets-widget-checkbox input[type="checkbox"] { margin-right:5px; }
.view-facet-source-services .view-header p, .view-facet-source-resources .view-header p { margin-bottom: 10px; }
.view-facet-source-services .views-exposed-form .btn-primary, .view-facet-source-resources .views-exposed-form .btn-primary { color:#fff; background-color:#3F2683; border-color:#3F2683; }
.view-facet-source-services .view-content .views-row, .view-facet-source-resources .view-content .views-row { background-color:#fff; padding:20px; margin-bottom:25px; }
.view-facet-source-services .view-content .views-row .views-field-nothing, .view-facet-source-resources .view-content .views-row .views-field-nothing { display:none; }
.view-facet-source-services .view-content .views-row h2, .view-facet-source-resources .view-content .views-row h3 { margin-top: 5px; }
.view-facet-source-services .view-content .views-row h2 a, .view-facet-source-resources .view-content .views-row h3 a { color:#3F2683; }
.view-facet-source-services .view-content .views-row h2 a:hover, .view-facet-source-resources .view-content .views-row h3 a:hover { color:#EC607E; text-decoration:none; }
.view-facet-source-services .view-content .views-row .views-field-field-tags, .view-facet-source-resources .view-content .views-row .views-field-field-tags { margin-top: 15px; }
.view-facet-source-resources .view-content .views-row .views-field-resource-type { margin-top:5px; }
.view-facet-source-services .view-content .views-row .views-field-field-tags a, .view-facet-source-services .view-content .views-row .views-field-field-tags .glyphicon, .view-facet-source-resources .view-content .views-row .views-field-field-tags a, .view-facet-source-resources .view-content .views-row .views-field-field-tags .glyphicon, .view-facet-source-resources .view-content .views-row .views-field-resource-type a, .view-facet-source-resources .view-content .views-row .views-field-resource-type .glyphicon { color:#EC607E; margin-left:7px; }
.view-facet-source-services .view-content .views-row .views-field-field-tags a:hover, .view-facet-source-resources .view-content .views-row .views-field-field-tags a:hover, .view-facet-source-resources .view-content .views-row .views-field-resource-type a:hover { color:#3F2683; text-decoration: none; }
.view-facet-source-resources .view-content .views-row .views-label-resource-type { color:#EC607E; font-weight:bold; }


@media (max-width:767px) {
	.phoneIcon img { display: none; }
	.pre-footer-stripe > .row, .pre-footer-stripe > .row > .container > .row { margin-left:0; margin-right: 0; }
	#block-getintouchlinks li { display:inline-block; }
	#block-getintouchlinks .navbar-nav, #block-getintouchlinks.navbar-nav { margin: 0; }
	#block-getintouchlinks li:last-child { border-right:none; }
	header.navbar .logo { padding-left:0; }
	.responsive-menu-toggle { float: right; }
	.responsive-menu-toggle-icon span.label { top:5px; left: -54px; color:#3F2683; }
	.responsive-menu-toggle-icon:before, .responsive-menu-toggle-icon:after, .responsive-menu-toggle-icon span.icon { background:#3F2683; border-radius: 2px; }
	.navbar-default .navbar-header #block-serenowebsitetheme-search { padding-bottom: 0; }

}

@media (min-width:768px) {
/* delete below */
	.salmonMenubar .navbar-brand { display:none; }
.salmonMenubar .navbar-default .nav>li>p, .salmonMenubar .navbar-default .nav>li>a { padding-top:8px; padding-bottom:8px; font-size:14px; line-height: 20px; }
.salmonMenubar .navbar-default .navbar-nav>li { color:#fff; }
.salmonMenubar .navbar-default .navContact>li { border-right:1px solid #FF9696; }
.salmonMenubar .navbar-default .navbar-nav>li.socialIcons>a { display:inline-block; padding-left:0; padding-right:10px; }
.salmonMenubar .navbar-default .navbar-nav>li.socialIcons>a:first-child { padding-left:10px; }
.mainMenu .container-fluid { padding:0; }
.mainMenu .navbar-form { text-align: right; margin-top: 15px; }
.mainMenu .navbar-right { margin-right: 0;padding-right: 15px; margin-top: 15px; }
.mainMenu .navbar-default .nav>li { padding-left:20px; }
.mainMenu .navbar-default .nav>li>a { padding:8px 10px; font-size:16px; line-height:22px; }


.navbar-default .navbar-header #block-serenowebsitetheme-search { float:right; width:400px; padding-right:0; }
.jumbotron { padding-top:0; padding-bottom:0; }
.jumbotron .container { padding-top:40px; padding-bottom:40px; }
.jumbotron .h1, .jumbotron h1 { font-size:42px; }
.jumbotron p { font-size:16px; }
.pre-footer-stripe .partnerLogo { width: 16.6666667%; }

	.path-frontpage .landing-page .content { display:flex; flex-direction:row; flex-wrap:wrap; }
	.path-frontpage .landing-page .content > div { display:flex; width:50%; }
	.path-frontpage .landing-page .content .CTAbox { width:100%; }

}

@media (min-width:768px) and (max-width:991px) {

.salmonMenubar .container-fluid { padding:0; }
.salmonMenubar .navbar-default .navbar-nav { padding-left: 20px; }
.salmonMenubar .navbar-right { border-top:1px solid #FF9696; float:left !important; width:100%; padding-left: 20px; }
.salmonMenubar .navbar-collapse { padding-right:0; padding-left:0; }
.salmonMenubar .navbar-default .navContact>li { border-right:1px solid #FF9696; }
.mainMenu .navbar-brand img { width:200px; }
.mainMenu .navbar-default .nav>li { padding-left:10px; }
.mainMenu .navbar-default .nav>li>a { padding:7px 10px; font-size:14px; line-height:20px; }

.upper-deck #block-secondarynavigation a, .upper-deck #block-getintouchlinks li p { padding-left:10px; padding-right:10px; }
.phoneIcon img { width:100px; }

}

@media (min-width:992px) {
.salmonMenubar .navbar-default .nav>li>p, .salmonMenubar .navbar-default .nav>li>a { padding:10px; font-size:13px; }
.salmonMenubar .navbar-default .navContact { margin-left: -15px; }
.jumbotron .container { background-image: url("../images/waves.png"); background-repeat: no-repeat; background-position: left top; background-size: contain; }
.jumbotron .h1, .jumbotron h1 { font-size: 50px; }
.phoneIcon { text-align:right; }
footer .footerLogo { width:210px; }
.pre-footer-stripe .partnerLogo { width: 12.5%; }

}

@media (min-width:1200px) {
.salmonMenubar .navbar-default .nav>li>p, .salmonMenubar .navbar-default .nav>li>a { padding:12px 15px; font-size:14px; }
.jumbotron .h1, .jumbotron h1 { font-size: 50px; }
footer .footerLogo { width:240px; }

}






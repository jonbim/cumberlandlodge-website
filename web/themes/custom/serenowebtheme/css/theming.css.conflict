


/*
Salmon:  #EC607E
Deep Purple:  #3F2683

font-family: 'Josefin Sans', sans-serif;
font-family: 'PT Sans', sans-serif;  400 and 700

*/

body { font-family: 'PT Sans', sans-serif; background:#F2F2F2; font-size:16px; }
h1, h2, h3, h4, h5, h6 { font-family: 'Josefin Sans', sans-serif; font-weight:400; letter-spacing:-1px; color:#3F2683; }
h2, .h2 { font-size: 32px; line-height: 1.2; }
h3, .h3 { font-size: 26px; line-height: 1.2em; }
p { margin-bottom:15px; }
.salmon { color:#EC607E; }
.button-white, .button-dark, .button-salmon { border: none; border-radius: 0; font-weight:700; transition: color 0.5s, background-color 0.5s; }
.button-white { color:#3F2683; }
.button-white:hover { color:#fff; background-color:#3F2683; }
.button-dark { color:#fff; background-color:#3F2683; }
.button-dark:hover { color:#3F2683; background-color:#fff; }
.button-salmon { color:#fff; background-color:#EC607E; }
.button-salmon:hover { color:#EC607E; background-color:#fff; }

.titleBar { background-color:#EC607E; padding:15px 30px 30px 30px; border-bottom-right-radius:25px; }
.titleBar h1 { color:#fff; font-size:48px; }
ol.breadcrumb glyphicon { color:#fff; }
ol.breadcrumb .active, ol.breadcrumb a { color:#fff; font-size:12px; }
.srn-header-not-front-page { margin-bottom: 50px; }
h1.page-header { margin-bottom: 0; padding-bottom:0; }
blockquote { padding: 10px 0; margin: 30px 40px 30px 20px; border-top: 5px solid #EC607E; border-bottom: 5px solid #EC607E; color: #EC607E; border-left:none;
}

/*  Menu and Masthead styles */
/* delete below */
.salmonMenubar { background-color:#EC607E; }
.salmonMenubar .navbar { margin-bottom:0; min-height:35px; }
.salmonMenubar .navbar-default .navbar-brand, .salmonMenubar .navbar-default .navbar-brand:focus, .navbar-default .navbar-brand:hover { color:#fff; }
.salmonMenubar .navbar-default { background-color:transparent; border:none; border-radius:0; }
.salmonMenubar .navbar-default .navbar-nav>li>a { color: #fff; }
.salmonMenubar .navbar-default .navbar-nav>li>a:hover { color: #000; }
.salmonMenubar .navbar-default .nav>li>p { position: relative; display: block; padding: 10px 15px; margin:0; }
.mainMenu .navbar-default { background-color:transparent; border:none; border-radius:0; }
.mainMenu .navbar-brand { height: auto; }
.mainMenu .navbar-brand img { width:230px; }
.mainMenu .navbar-default .navbar-nav>li>a { color:#3F2683; }
.mainMenu .navbar-default .navbar-nav>li>a:hover { color:#EC607E; text-decoration:none }
.mainMenu .navbar-default .navbar-nav>li.menu-item--active-trail>a { background-color:green; border-bottom:3px solid #3F2683; }
.mainMenu .navbar-default .navbar-nav>li.menu-item--active-trail>a:hover { background-color:blue; color: #000; }
.mainMenu .navbar-default .navbar-nav>li:last-child>a { background-color:#3F2683; color:#fff; }
.mainMenu .navbar-default .navbar-nav>li:last-child>a:hover { background-color:#EC607E; color:#000; }

header #search-block-form .btn-primary { color:#fff; background-color:#3F2683; border-color:#3F2683; }
#block-getintouchlinks li { border-right:1px solid #FF9696; }
#block-getintouchlinks li a { text-decoration: none; }
#block-secondarynavigation li a:hover { text-decoration: none; background-color:transparent; color:#000; }
.navbar-default .navbar-header li a:hover { text-decoration: none; }
.navbar-default .navbar-header li.menu-item--active-trail a:hover { background-color:transparent; color: #EC607E; }
.navbar-default .navbar-header li:last-child a:hover { background-color:#EC607E; color: #fff; }
.navbar-default .navbar-header li:last-child.menu-item--active-trail a { background-color:#3F2683; border-bottom: none; }
.navbar-default .navbar-header li:last-child.menu-item--active-trail a:hover { background-color:#EC607E; }

